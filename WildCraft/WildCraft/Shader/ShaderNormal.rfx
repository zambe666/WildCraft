    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
int    gDirLightNum = 0;      // 점광원 개수
float3 gDirLightNormal[4];    // 직사광선 

struct PS_INPUT
{
   float3 mUV       : TEXCOORD0;
   float3 mViewDir : TEXCOORD1;
   float3 mDiffuse : TEXCOORD2;
   float3 mReflection: TEXCOORD3;
   float3 mWorldNormal : NORMAL0;
};

sampler2D DiffuseSampler;

float4 ps_main(PS_INPUT Input) : COLOR0
{
   float3 reflection = normalize(Input.mReflection);
   float3 viewDir = normalize(Input.mViewDir);
   
   float4 albedo = tex2D(DiffuseSampler, Input.mUV);
   for(int i = 0; i<gDirLightNum; i++)
   {
      float3 dirLight = normalize(gDirLightNormal[i];
      Input.mDiffuse += dot(- lightDir, Input.mWorldNormal);
   }
   float3 diffuse = albedo.rgb * saturate(Input.mDiffuse);
   
   float3 specular = 0;
   if(diffuse.x > 0)
   {
      specular = saturate(dot(reflection, -viewDir));
      specular = pow(specular,20.0f);
      
      //float4 specularIntensity = tex2D(SpecularSampler, Input.mUV);
      //specular *= specularIntensity.rgb * gLightColor);
   }
   float ambient = float3(0.1f,0.1f,0.1f) * albedo;
   
   return( float4( ambient + diffuse + specular, 1.0f ) );
   
}



]]></RmShaderCode>
