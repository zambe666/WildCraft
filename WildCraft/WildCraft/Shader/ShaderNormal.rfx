    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matWorld;             // 월드 
float4x4 matViewProjection;    // 뷰프로젝 

int    gPointLightNum = 0;      // 점광원 개수
float4 gPointLightPosition[4]; // 점광원 4개까지만 지원
float4 gCameraPosition;         // 카메라 위치 

struct VS_INPUT 
{
   float4 mPosition : POSITION0;
   float2 mUV       : TEXCOORD0;
   float3 mNormal   : NORMAL0; 
};

struct VS_OUTPUT 
{
   float4 mPosition    : POSITION0;
   float3 mUV          : TEXCOORD0;
   float3 mViewDir     : TEXCOORD1;
   float3 mDiffuse     : TEXCOORD2;
   float3 mReflection  : TEXCOORD3;
   float3 mWorldNormal : NORMAL0;
};

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output = (VS_OUTPUT)0;
   
   Output.mPosition = mul(Input.mPosition, matWorld);
   
   // 월드 시점에서 판단 하자 
   float3 worldNormal = mul(Input.mNormal, (float3x3)matWorld);
   Output.mWorldNormal = normalize(worldNormal);
   
   float3 viewDir = Output.mPosition.xyz - gCameraPosition.xyz;
   Output.mViewDir = normalize(viewDir);
   
   for(int i = 0; i< gPointLightNum; i++)      // 점광원 수만큼 
   {
     float3 lightDir = Output.mPosition.xyz - gPointLightPosition[i].xyz;
     lightDir = normalize(lightDir);
     
     Output.mDiffuse += dot(- lightDir, Output.mWorldNormal);
     Output.mReflection += reflect(lightDir, Output.mWorldNormal);
   }
   Output.mPosition = mul( Input.mPosition, matViewProjection );
   
   return( Output ); 
}



]]></RmShaderCode>
